//Задание 14
#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double a, y;
    cout << "Введите значение a: ";
    cin >> a;
    y = (a * a) + (1 / sqrt((a * a) + 1));
    cout << "Значение функции y = a^2 + 1/√(a^2 + 1) при a = " << a << " равно " << y << endl;
    return 0;
}
