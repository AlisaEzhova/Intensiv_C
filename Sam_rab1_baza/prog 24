//Задание 24
#include <iostream>
using namespace std;

int main() {
    int population;
    double area, density;

    cout << "Введите количество жителей в государстве: ";
    cin >> population;

    cout << "Введите площадь территории государства (в квадратных километрах): ";
    cin >> area;

    density = population / area; // вычисляем плотность населения

    cout << "Плотность населения в государстве: " << density << " чел/км^2" << endl;

    return 0;
}
