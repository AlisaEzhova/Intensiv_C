//Задание 30
#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double x, y, z, q;
    cout << "Введите значение x: ";
    cin >> x;
    cout << "Введите значение y: ";
    cin >> y;
    z = x + (2 + y) / (pow(x, 2) / y + 1 / sqrt(pow(x, 2) + 10));
    q = 2.8 * sin(x) + abs(y);
    cout << "Значение функции z равно " << z << endl;
    cout << "Значение функции q равно " << q << endl;
    return 0;
}
