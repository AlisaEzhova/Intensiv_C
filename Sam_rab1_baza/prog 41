//Задание 41 а), б), в)
#include <iostream>

int main() {
    double a = 2.5;
    double a2 = a * a;
    double a4 = a2 * a2;
    double a5 = a4 * a;
    double a7 = a5 * a2;

    std::cout << "a^4 = " << a4 << std::endl;
    std::cout << "a^5 = " << a5 << std::endl;
    std::cout << "a^7 = " << a7 << std::endl;

    return 0;
}

//Задание 41 г), д), е)
#include <iostream>

using namespace std;

int main() {
     double a;
     cout << "Введите вещественное число a: ";
     cin >> a;
     double a2 = a * a;
     double a4 = a2 * a2;
     double a8 = a4 * a4;
     cout << "a^8 = " << a8 << endl;

return 0;
}


д)
#include <iostream>

using namespace std;

int main() {
double a;
cout << "Введите вещественное число a: ";
cin >> a;
double a3 = a * a2;
double a9 = a3 * a3 * a3;
cout << "a^9 = " << a9 << endl;

return 0;

}


