//Задание 23
#include <iostream>
using namespace std;

int main() {
    double volume, mass, density;

    cout << "Введите объем тела (в кубических метрах): ";
    cin >> volume;

    cout << "Введите массу тела (в килограммах): ";
    cin >> mass;

    density = mass / volume; // вычисляем плотность

    cout << "Плотность материала тела: " << density << " кг/м^3" << endl;

    return 0;
}
