//Задание 19
#include <iostream>

using namespace std;

int main() {
    double a, volume, area;
    cout << "Введите длину ребра куба: ";
    cin >> a;
    volume = a * a * a;
    area = 4 * a * a;
    cout << "Объем куба: " << volume << endl;
    cout << "Площадь боковой поверхности куба: " << area << endl;
    return 0;
}
